defaults 

account gmail
host smtp.gmail.com
port 587
protocol smtp
auth on
from ghostbar38@gmail.com
user ghostbar38@gmail.com
passwordeval gpg -d ~/.msmtp.gmail.pass.asc
tls on
tls_nocertcheck

account rivco
host smtp.gmail.com
port 587
protocol smtp
auth on
from joseluis@rivco.net
user joseluis@rivco.net
passwordeval gpg -d ~/.msmtp.rivco.pass.asc
tls on
tls_nocertcheck

account default: gmail
